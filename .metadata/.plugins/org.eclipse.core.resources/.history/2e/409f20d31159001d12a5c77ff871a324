package Important;


public class Reverse_Linked_List {

	public static void main(String[] args) {

		SinglyLinearList list = new SinglyLinearList();
		
		list.addNode(11);
		list.addNode(201);
		list.addLast(66);
		list.addNode(22);
		list.addNode(33);
		list.addNode(44);
		list.addFirst(101);
		list.addPos(33, -1);
		list.addNode(55);
		
		list.display();

		System.out.println("\nNew LIst");
		list.reverse();
		
X	}

}
