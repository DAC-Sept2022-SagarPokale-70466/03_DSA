package _03_Circular_Queue;
public class CircularQueue {
	private int[] arr;
	private int front;
	private int rear;
	private final int SIZE;
	
	public CircularQueue(int size) {
		SIZE = size;
		arr = new int[SIZE];
		front = -1;
		rear = -1;
	}
	
}
